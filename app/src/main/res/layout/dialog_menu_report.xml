<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="80dp"
    android:layout_height="34dp"
    android:background="#33FFFFFF"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/iv_report"
        android:layout_width="80dp"
        android:layout_height="34dp"
        android:background="@color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font_notosanskr_regular"
        android:includeFontPadding="false"
        android:text="@string/popup_menu_report"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:lineSpacingExtra="3sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_report"
        app:layout_constraintEnd_toEndOf="@+id/iv_report"
        app:layout_constraintStart_toStartOf="@+id/iv_report"
        app:layout_constraintTop_toTopOf="@+id/iv_report" />


</androidx.constraintlayout.widget.ConstraintLayout>